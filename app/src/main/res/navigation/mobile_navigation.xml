<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.frc.frcinnovationptsd.ui.HeartMonitorFragment"
        android:label="Heart Rate"
        tools:layout="@layout/heart_monitor" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.frc.frcinnovationptsd.ui.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.frc.frcinnovationptsd.ui.EarPieceFragment"
        android:label="Audio Monitoring"
        tools:layout="@layout/ear_piece" />
</navigation>